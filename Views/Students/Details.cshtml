<div>
    <form>
        <a id="links" asp-action="Index">
            <img class="mb-2 mr-2" src="~/css/images/back.png" width="35" height="35" />
            Powrót do listy
        </a>
        <a id="links" asp-action="Edit" asp-route-id="@Model.id">
            <img class="ml-4 mr-2" src="~/css/images/edit.png" width="30" height="30" />
            Edytuj
        </a>
    </form>
    
</div>
