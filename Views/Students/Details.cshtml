    </header>
    <hr />
    <dl class="row">
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.names)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.names)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.surname)
